{
  "name": "vasily-kartashov/amazon-alexa-php",
  "description": "Amazon Alexa interface for PHP",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Vasily Kartashov",
      "email": "info@kartashov.com"
    },
    {
      "name": "Mathias Hansen",
      "email": "me@codemonkey.io"
    },
    {
        "name": "Emanuele Corradini",
        "email": "korra88@gmail.com"
    },
    {
      "name": "Jakub Suchy",
      "email": "info@jsuchy.cz"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "sort-packages": true
  },
  "require": {
    "php": "^7 || ^8",
    "ext-curl": "@stable",
    "ext-json": "@stable",
    "ext-openssl": "@stable",
    "psr/http-message": "@stable"
  },
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "@stable",
    "phpunit/phpunit": "^6 || ^7 || ^8",
    "squizlabs/php_codesniffer": "@stable",
    "vimeo/psalm": "@stable",
    "symfony/polyfill-mbstring": "<=1.20.0"
  },
  "autoload": {
    "psr-4": {
      "Alexa\\" : "src/"
    }
  }
}
